{
    "name": "pureclarity/php-sdk",
    "description": "PureClarity Intelligent Personalization - PHP SDK",
    "type": "library",
    "license": ["OSL-3.0"],
    "version": "2.1.1",
    "require": {
        "php": "^5.6|^7.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~6.0",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Pureclarity\\Api\\": "src/PureClarity/"
        },
        "files": [
            "src/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pureclarity\\Tests\\": "tests/PureClarity/"
        }
    }
}
